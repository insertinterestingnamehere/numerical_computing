\newcommand*\crop[1][cam,noaxes]{%
  \@for\CROP@@:=#1\do{\CROP@execopt\CROP@@}}
\newcommand*\CROP@execopt[1]{%
  \def\CROP@##1=##2=##3\@nil{\def\CROP@{##1}\def\CROP@@{##2}}%
